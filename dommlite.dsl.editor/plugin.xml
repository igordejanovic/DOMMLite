<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="dommlite.editor.DommliteEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="dl"
            icon="icons/file.gif"
            id="dommlite.editor.DommliteEditor"
            name="dommlite Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="dommlite.wizards.NewDommliteProjectWizard"
			icon="icons/template.gif"
			id="dommlite.wizards.NewDommliteProjectWizard"
			name="dommlite Project"
			project="true">
		</wizard>
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="dommlite.wizards.NewDommliteFileWizard"
			icon="icons/file.gif"
			id="dommlite.wizards.NewDommliteFileWizard"
			name="dommlite Editor file">
		</wizard>
	</extension>
</plugin>
